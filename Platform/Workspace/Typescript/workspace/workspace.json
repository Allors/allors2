{
  "version": 1,
  "projects": {
    "workspace-meta": {
      "root": "libs/workspace/meta",
      "sourceRoot": "libs/workspace/meta/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/workspace/meta/tsconfig.lib.json",
              "libs/workspace/meta/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/workspace/meta/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/workspace/meta/jest.config.js",
            "tsConfig": "libs/workspace/meta/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "workspace-domain": {
