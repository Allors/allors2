    "domain": {
      "root": "libs/domain",
      "sourceRoot": "libs/domain/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/domain/tsconfig.lib.json",
              "libs/domain/tsconfig.spec.json"
