      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/workspace/meta/tsconfig.lib.json",
              "libs/workspace/meta/tsconfig.spec.json"
            ],
