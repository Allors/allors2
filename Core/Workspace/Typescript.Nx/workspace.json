{
  "version": 1,
  "projects": {
    "meta": {
      "root": "libs/meta",
      "sourceRoot": "libs/meta/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/meta/tsconfig.lib.json",
              "libs/meta/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/meta/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/meta/jest.config.js",
            "tsConfig": "libs/meta/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
