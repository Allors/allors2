@page "/"
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Blazor.Client;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    <title>Blazor (Server Side)</title>
    <base href="~/" />
    <link rel="stylesheet" href="_content/BlazorStrap/css/submenu.css" />
</head>
<body>
    <app>
        @* Remove the following line of code to disable prerendering *@
        @(await Html.RenderStaticComponentAsync<App>())
    </app>

    <script src="_framework/blazor.server.js"></script>

    <!-- The below two files are needed by BlazorStrap -->
    <script src="_content/BlazorStrap/blazorStrap.js"></script>
    <script src="_content/BlazorStrap/popper.min.js"></script>

</body>
</html>
