{
  "name": "material",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "node ./postinstall.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "tslint --fix --project ./src/tsconfig.app.json",
    "build-stats": "ng build --aot --optimization --stats-json",
    "stats": "webpack-bundle-analyzer dist/material/stats-es2015.json",
    "autotest": "npx -p typescript tsc --project ./autotest/tsconfig.autotest.json && node ./out-tsc/autotest/autotest/main  && npx rimraf ./out-tsc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "^10.0.2",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/material": "^10.0.2",
    "@angular/material-moment-adapter": "~10.0.2",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "bootstrap": "^4.5.0",
    "easymde": "^2.11.0",
    "marked": "^1.1.1",
    "moment": "2.27.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "turndown": "^6.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.2.0",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@types/codemirror": "0.0.97",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/marked": "^1.1.0",
    "@types/node": "^12.11.1",
    "@types/turndown": "^5.0.0",
    "browser-env": "^3.3.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-trx-reporter": "^0.4.3",
    "lnk": "^1.1.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
