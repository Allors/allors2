    "paths": {
      "@baseMeta": ["../../../../Base/Workspace/Typescript/Meta/src/allors/meta/base/index"],
      "@baseDomain": ["../../../../Base/Workspace/Typescript/Domain/src/allors/domain/base/index"],
      "@basePromise": ["../../../../Base/Workspace/Typescript/Promise/src/allors/domain/promise/*"],

      "@baseAngular/*": ["../../../../Base/Workspace/Typescript/Angular/src/allors/angular/base/*"],
      "@baseMaterial/*": ["../../../../Base/Workspace/Typescript/Material/src/allors/material/base/*"],
      "@baseCovalent/*": ["../../../../Base/Workspace/Typescript/Covalent/src/allors/covalent/base/*"],

      "@appsDomain": ["../Domain/src/allors/domain/apps/index"],

      "@appsIntranet/*": ["./src/allors/intranet/apps/*"],

      "@generatedMeta/*": ["../Domain/src/allors/meta/generated/*"],
      "@generatedDomain/*": ["../Domain/src/allors/domain/generated/*"],

      "@allors": ["./src/allors/index"],

      "*": ["./node_modules/*"]
    }
  }
}
