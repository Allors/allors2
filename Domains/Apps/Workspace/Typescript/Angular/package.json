    "sync-apps-domain": "sync-files ../Domain/src/allors/domain/apps src/allors/domain/apps",
    "syncback": "npm run syncback-base && npm run syncback-apps",
    "syncback-base": "npm run syncback-base-meta && npm run syncback-base-domain && npm run syncback-base-angular && npm run syncback-base-material && npm run syncback-base-covalent",
