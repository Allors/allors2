    "syncback-apps": "npm run syncback-apps-domain",
    "syncback-apps-domain": "sync-files src/allors/domain/apps ../Domain/src/allors/domain/apps",
    "teamcity": "ng test --watch=false --reporters=teamcity",
    "teamcity-e2e": "ng e2e --config ./protractor.conf.teamcity.js"
