{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "baseUrl": "./",
    "module": "commonjs",
    "target": "es5",
    "types": [
      "jasmine",
      "node"
    ],
    "paths": {
      "@allors/generated-meta/*": ["./generated/meta/*"],
      "@allors/generated-domain/*": ["./generated/domain/*"],
      "@allors": ["./allors/index"],
      "*": ["../node_modules/*"]
    }
  },
  "files": [
    "test.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
