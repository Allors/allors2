{
  "name": "angular-material",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sync": "npm run sync-meta && npm run sync-domain && npm run sync-angular",
    "sync-meta": "sync-files ../Meta/src/allors/meta/base src/allors/meta/base",
    "sync-domain": "sync-files ../Domain/src/allors/domain/base src/allors/domain/base",
    "sync-angular": "sync-files ../Angular/src/allors/angular/base src/allors/angular/base",
    "syncback": "npm run syncback-meta && npm run syncback-domain && npm run syncback-angular",
    "syncback-meta": "sync-files src/allors/meta/base ../Meta/src/allors/meta/base",
    "syncback-domain": "sync-files src/allors/domain/base ../Domain/src/allors/domain/base",
    "syncback-angular": "sync-files src/allors/angular/base ../Angular/src/allors/angular/base",
    "postinstall": "npm run sync"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "sync-files": "^1.0.3"
  }
}
