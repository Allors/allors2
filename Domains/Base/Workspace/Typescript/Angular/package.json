{
  "name": "angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "sync": "npm run sync-meta && npm run sync-domain",
    "sync-meta": "sync-files ../Meta/src/allors/meta/base src/allors/meta/base",
    "sync-domain": "sync-files ../Domain/src/allors/domain/base src/allors/domain/base",
    "syncback": "npm run syncback-meta && npm run syncback-domain",
    "syncback-meta": "sync-files src/allors/meta/base ../Meta/src/allors/meta/base",
    "syncback-domain": "sync-files src/allors/domain/base ../Domain/src/allors/domain/base",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "teamcity": "ng test --watch=false --reporters=teamcity",
    "teamcity-e2e": "ng e2e --config ./protractor.conf.teamcity.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.2.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-reporters": "^2.2.1",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-teamcity-reporter": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "~5.1.2",
    "sync-files": "^1.0.3",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
