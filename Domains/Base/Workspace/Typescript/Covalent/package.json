{
  "name": "apps",
  "version": "1.0.0",
  "private": true,
  "description": "Allors Apps",
  "keywords": [
    "allors",
    "apps",
    "reusable"
  ],
  "scripts": {
    "start": "ng serve",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "test": "ng test --code-coverage --single-run",
    "sync": "npm run sync-meta && npm run sync-domain && npm run sync-angular && npm run sync-material",
    "sync-meta": "sync-files ../Meta/src/allors/meta/base src/allors/meta/base",
    "sync-domain": "sync-files ../Domain/src/allors/domain/base src/allors/domain/base",
    "sync-angular": "sync-files ../Angular/src/allors/angular/base src/allors/angular/base",
    "sync-material": "sync-files ../Material/src/allors/material/base src/allors/material/base",
    "syncback": "npm run syncback-meta && npm run syncback-domain && npm run syncback-angular && npm run syncback-material",
    "syncback-meta": "sync-files src/allors/meta/base ../Meta/src/allors/meta/base",
    "syncback-domain": "sync-files src/allors/domain/base ../Domain/src/allors/domain/base",
    "syncback-angular": "sync-files src/allors/angular/base ../Angular/src/allors/angular/base",
    "syncback-material": "sync-files src/allors/material/base ../Material/src/allors/material/base"
  },
  "repository": "https://github.com/allors/allors2.git",
  "license": "LGPL",
  "author": "Allors bvba",
  "dependencies": {
    "@angular/animations": "^4.2.0",
    "@angular/common": "^4.2.0",
    "@angular/compiler": "^4.2.0",
    "@angular/core": "^4.2.0",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "^4.2.0",
    "@angular/http": "^4.2.0",
    "@angular/material": "2.0.0-beta.6",
    "@angular/platform-browser": "^4.2.0",
    "@angular/platform-browser-dynamic": "^4.2.0",
    "@angular/platform-server": "^4.2.0",
    "@angular/router": "^4.2.0",
    "@covalent/core": "1.0.0-beta.5",
    "@covalent/dynamic-forms": "1.0.0-beta.5",
    "@covalent/highlight": "1.0.0-beta.5",
    "@covalent/http": "1.0.0-beta.5",
    "@covalent/markdown": "1.0.0-beta.5",
    "@swimlane/ngx-charts": "^5.3.0",
    "core-js": "^2.4.1",
    "d3": "^4.9.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.10.0",
    "rxjs": "^5.4.0",
    "showdown": "1.6.4",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.1.1",
    "@angular/compiler-cli": "^4.2.0",
    "@types/hammerjs": "2.0.30",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "2.53.36",
    "codelyzer": "~3.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "phantomjs-prebuilt": "2.1.7",
    "protractor": "~5.1.0",
    "ts-node": "~2.1.0",
    "tslint": "^5.2.0",
    "typescript": "~2.3.1",
    "sync-files": "^1.0.3"
  }
}
