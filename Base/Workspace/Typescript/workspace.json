    "intranet": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/intranet",
      "sourceRoot": "apps/intranet/src",
      "prefix": "allors",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/intranet",
            "index": "apps/intranet/src/index.html",
            "main": "apps/intranet/src/main.ts",
            "polyfills": "apps/intranet/src/polyfills.ts",
            "tsConfig": "apps/intranet/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/intranet/src/favicon.ico",
              "apps/intranet/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/intranet/src/styles.scss",
              "./node_modules/easymde/dist/easymde.min.css"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "zone.js",
              "markdown.component",
              "easymde",
              "codemirror",
              "codemirror-spell-checker"
            ],
            "preserveSymlinks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/intranet/src/environments/environment.ts",
                  "with": "apps/intranet/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "intranet:build",
            "proxyConfig": "apps/intranet/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "intranet:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "intranet:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/intranet/tsconfig.app.json",
              "apps/intranet/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/intranet/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/intranet/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/intranet"
          ]
        }
      }
    },
